{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "paths": {
      "@/*": [
        "./frontend/*"
      ],
      "@firebase/auth": ["./node_modules/@firebase/auth/dist/index.rn.d.ts"]
      
    }
  },
  "include": [
    "frontend/**/*.ts",
    "frontend/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "App.tsx"
  ]
}